import requests
import json

from time import sleep
import keyboard




import collections
import urllib, json

url = "http://127.0.0.1:8000/api/address"
response = urllib.request.urlopen(url)
data = json.loads(response.read())

last =data[-1]

auth = last.get("auth")
#print(auth)    










keyboard.write(auth)
keyboard.press_and_release('enter')






